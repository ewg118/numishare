         </geographic>
         <obverse>
            <type>
               <description xml:lang="en">Laureated head of Hadrian facing right.</description>
            </type>
            <legend>II IMP(ERATOR} HADRI(ANUS)</legend>
         </obverse>
         <reverse>
            <type>
               <description xml:lang="en">Turreted figure standing in hexastyle temple
				holding globe and sphere.</description>
            </type>
            <legend>CO(LONIA) AE(LIA) CAP(ITOLINA)</legend>
            <persname xlink:type="simple" xlink:role="deity">Astarte</persname>
         </reverse>
      </typeDesc>
